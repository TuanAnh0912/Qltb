@model List<qltb.WebApp.Models.StorageRoomViewModel.ThietBiTrongPhongViewModel>
@{
    Layout = null;
}
@foreach (var equip in Model)
{
<tr>
    <td>
        <input hidden name="KhoThietBiId" value="@equip.KhoThietBiId" />
        <input hidden name="ThietBiId" value="@equip.ThietBiId" />
        @equip.MaThietBi
    </td>
    <td class="">@equip.TenThietBi</td>
    <td class="text-center">
        <input type="number" value="1" onchange="changeSoluong(this)" />
    </td>
    <td class="text-center">0</td>
    <td class="text-center">0</td>
    <td class="text-center"><a href="#" onclick="RemoveThietBi(this)">Xóa</a></td>
</tr>
}

